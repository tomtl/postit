<%= render 'shared/errors', obj: @user %>

<div class='well'>
  <%= form_for @user do |f| %>
    <div class='control-group'>
      <%= f.label :username %>
      <%= f.text_field :username %>
    </div>
    <div class='control-group'>
      <%= f.label :password %>
      <%= f.text_field :password %>
    </div>
    <%= f.submit 'Register', class:'btn btn-primary' %>
  <% end %>
</div>